    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-bearer-token": "^2.4.0",
    "express-validator": "^6.6.1",
    "faker": "^4.1.0",
    "helmet": "^4.0.0",
    "http": "0.0.1-security",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "path": "^0.12.7",
    "pg": "^8.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.0"
  },
